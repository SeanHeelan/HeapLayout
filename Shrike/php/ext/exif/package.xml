   <file role="test" name="tests/exif004.phpt"/>
   <file role="test" name="tests/test4.jpg"/>
   <file role="test" name="tests/exif005.phpt"/>
   <file role="test" name="tests/test5.jpg"/>
   <file role="test" name="tests/exif006.phpt"/>
